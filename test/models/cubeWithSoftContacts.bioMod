version 4

// DEFINITION DU SEGMENTS

// Seg1
segment Seg1
    translations	xyz
    rotations	y
    rangesQ  -0.70 2.30
             -10 10
             -3    3
             -pi   pi
    mass 1
    inertia
        1 0 0
        0 1 0
        0 0 1
    com 0.1 0.1 0.1
        mesh 0 -1 -1
        mesh 0 0 -1
        mesh 0 0 0
        mesh 0 -1 0
        mesh 0 -1 -1
        mesh 1 -1 -1
        mesh 1 0 -1
        mesh 0 0 -1
        mesh 1 0 -1
        mesh 1 0 0
        mesh 0 0 0
        mesh 1 0 0
        mesh 1 -1 0
        mesh 0 -1 0
        mesh 1 -1 0
        mesh 1 -1 -1
endsegment

// Soft contacts
    softcontact Contact1
        parent Seg1
        type sphere
        position 2 3 4
        radius 5
        stiffness 6
        damping 7
    endsoftcontact
    
    softcontact Contact2
        parent Seg1
        type sphere
        position 8 9 10
        radius 11
        stiffness 12
        damping 13
    endsoftcontact
